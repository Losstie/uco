/**index.wxss**/
view {
  color:var(--themeColor);
}
/*  */
swiper{
  width: 750rpx;
  height: 340rpx;
}
swiper>image{
  width: 100%;
}

/* 导航 开始*/
.index_cate { display: flex }
.index_cate>navigator{
  flex:1;
  padding:20rpx;
}
.index_cate image {
  width: 100%;
}
/* 导航 结束 */

/* 楼层 开始*/
.index_floor, .floor_group, .floor_title { padding: 10rpx 0;}

.floor_title image{
  width: 100%;
}
.floor_list {
  overflow: hidden;
}
.floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
}

.floor_list navigator image{
  width: 100%;
  height: 100%;
}

.floor_list navigator:nth-last-child(-n+4){
  height: 27.72711207vw;
  border-left:10rpx solid #fff;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}